{
  "private": false,
  "name": "@mblabs/docs-shared",
  "version": "0.0.1",
  "description": "Documentation partials shared between project documentation partial repositories",
  "license": "MIT",
  "author": "Brian Zalewski <brian@megabyte.space> (https://megabyte.space)",
  "homepage": "https://megabyte.space",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorManhattan/docs-shared.git"
  },
  "bugs": {
    "email": "help@megabyte.space",
    "url": "https://gitlab.com/megabyte-labs/documentation/shared/-/issues"
  },
  "type": "module",
  "scripts": {
    "help": "task --list",
    "prepare": "task common:husky",
    "repair": "bash <(curl -sS https://install.doctor/repair)",
    "setup": "",
    "start": "bash .config/start.sh"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  },
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "@washingtondc/development": "^1.0.2",
    "@washingtondc/prettier": "^1.0.0",
    "@washingtondc/release": "^0.0.2",
    "eslint-config-strict-mode": "^1.0.28",
    "semantic-release-config": "^0.0.12",
    "sleekfast": "^0.0.1"
  },
  "keywords": [
    "common",
    "docs",
    "documentation",
    "mblabs",
    "megabytelabs",
    "npm",
    "package",
    "partials",
    "professormanhattan",
    "shared",
    "washingtondc"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "blueprint": {
    "description": "Documentation partials shared between project documentation partial repositories",
    "docs": {
      "link": "https://megabyte.space/docs/documentation/shared"
    },
    "github_id": "R_kgDOGep-rw",
    "gitlab_group_id": "11096751",
    "gitlab_project_id": "27548086",
    "group": "documentation",
    "name": "Shared Docs",
    "overview": "This repository contains documentation partials that are propagated downstream to project-type specific documentation partial repositories. For example, in this repository there may be a partial titled `contributing.md` which contains sponsorship links. When those links are updated, the partials in this repository are propagated to the project-type specific repositories (e.g. NPM package documentation, Dockerfile project documentation). The project-type specific documentation partials are then used by the projects that they are intended for. In this way, we can update all the links to Patreon across all of our repositories by editting in just one spot.",
    "repository": {
      "github": "https://github.com/ProfessorManhattan/docs-shared",
      "gitlab": "https://gitlab.com/megabyte-labs/documentation/shared"
    },
    "slug": "docs-shared",
    "subgroup": "shared",
    "title": "Shared Documentation Partials"
  },
  "changelog": {
    "displayTypes": [
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert"
    ],
    "showAuthor": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "helpUrl": "https://megabyte.space/docs/contributing/commits"
  },
  "eslintConfig": {
    "extends": "eslint-config-strict-mode",
    "rules": {
      "import/no-extraneous-dependencies": "warn",
      "max-len": "warn",
      "max-lines": "warn"
    }
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/megabytelabs"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/ProfessorManhattan"
    }
  ],
  "lint-staged": {
    "**/!(logo).(jpg|png)": [
      "task image:compress --"
    ],
    "**/*": [
      "task lint:any --"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.sh": [
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "ntl": {
    "descriptions": {
      "help": "Display the Bodega Taskfile.yml interactive help menu",
      "prepare": "Hook, triggered by running npm install, that runs logic such as installing pre-commit hooks",
      "repair": "Fixes basic issues in projects that are having trouble running basic commands like 'task start'",
      "start": "Entrypoint for new developers that installs requirements and then runs 'task start'"
    }
  },
  "prettier": "@washingtondc/prettier",
  "release": {
    "branches": [
      "main",
      "master",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "semantic-release-config"
  }
}
